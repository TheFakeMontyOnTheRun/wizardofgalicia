CXX = em++
CXXFLAGS = -g  --preload-file res --use-preload-plugins -Iinclude -std=c++1y

OBJS = src/CActor.o  src/CBaphomet.o src/CCuco.o src/CDoorway.o  src/CGame.o src/CMap.o src/CWizard.o  src/IMapElement.o  src/Vec2i.o src/CFireball.o src/CRelic.o SDLVersion/CSDLRelativeRenderer.o SDLVersion/main.o


TARGET = wizard.html

$(TARGET):	$(OBJS)
	$(CXX) -o $(TARGET) $(OBJS) $(CXXFLAGS)

all:   $(TARGET)

clean:
	rm -f $(OBJS) $(TARGET)
	rm wizard.data
	rm wizard.js